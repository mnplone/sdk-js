{
	"name": "@mnplone/sdk",
	"version": "0.1.0-beta.1",
	"description": "SDK for mnpl.one",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/esm/main.js",
	"types": "dist/esm/main.d.ts",
	"exports": {
		".": "./dist/esm/main.js",
		"./dev": "./dist/esm/dev.js"
	},
	"engines": {
		"node": ">=21"
	},
	"dependencies": {
		"@extws/client": "github:extws-team/client#b308dfed0becd80747ec638fadcb09ba4737d073",
		"valibot": "^0.42.1"
	},
	"devDependencies": {
		"@kirick/eslint-config": "^0.1.30",
		"@types/node": "^22.2",
		"eslint": "9.8.0",
		"typescript": "5.5.4",
		"vitest": "2.1.2"
	},
	"scripts": {
		"build": "rm -r dist ; bun run build:ts",
		"build:ts": "rm -r dist/esm ; tsc",
		"lint": "eslint . && tsc --skipLibCheck --noemit",
		"publish:npm": "bun run lint && bun run build && bun run test && npm publish",
		"test": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun test --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mnplone/sdk-js.git"
	},
	"keywords": [
		"mnplone",
		"sdk"
	],
	"author": "Daniil Kirichenko (https://twitter.com/kirickme)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mnplone/sdk-js/issues"
	},
	"homepage": "https://github.com/mnplone/sdk-js#readme"
}
